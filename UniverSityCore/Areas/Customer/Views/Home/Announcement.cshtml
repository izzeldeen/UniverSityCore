@{
}

@model IEnumerable<UniverSity.Models.Announcement>

@{
    ViewData["Title"] = "Privacy Policy";
    Layout = Layout = "/Views/Shared/_Layout.cshtml";
}


    <div class="col-12 text-right p-5">

        <h4 class=" text-info">الأعلانات  </h4>
        <hr />

        @if (Model.Count() > 0)
        {
            @foreach (var item in Model)
            {
                <h5 class="text-right text-info">@item.Subject</h5>

                <p class="text-right">@Html.Raw(item.Descrption)   </p>

            }
        }

        else
        {
         <p>لايوجد اعلانات</p>
        }






    </div>
