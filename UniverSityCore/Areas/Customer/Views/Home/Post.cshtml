@model IEnumerable<UniverSity.Models.Post>

@{
    ViewData["Title"] = "Post";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="col-12 text-right p-5">

    <h4 class=" text-info">المدونات   </h4>
    <hr />

    @if (Model.Count() > 0)
    {
        @foreach (var item in Model)
        {
            <h5 class="text-right ">@item.Subject</h5>

            <p>
                @Html.Raw(item.Description)

            </p>

             
            
            <hr />

        }
    }

    else
    {
        <p> لا يوجد مدونات </p>
    }





</div>
