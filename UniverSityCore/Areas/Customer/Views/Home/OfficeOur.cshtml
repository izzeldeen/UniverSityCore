@model IEnumerable<UniverSity.Models.OfficeHours>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@{
    ViewData["Title"] = "Index";

}
<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
<link rel="stylesheet" href="~/css/site.css" />
<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.0.13/css/all.css" integrity="sha384-DNOHZ68U8hZfKXOrtjWvjxusGo9WQnrNx2sqG0tfsghAvtVlRW3tvkXWZh58N9jp"
      crossorigin="anonymous">






<div class="row">

    <div class="col-12 text-right">
        <h3 class="text-info mr-5">قائمة الساعات المكتبية </h3>

    </div>


</div>
<hr />
<div>

    @if (Model.Count() > 0)
    {
        <table class="table table-striped border table flex-row-reverse">
            <tr class="table-secondary">


                <th></th>
                <th></th>
                <th></th>

                <th>
                    @Html.DisplayNameFor(model => model.OfficeNum)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.EndHour)

                </th>
                <th>
                    @Html.DisplayNameFor(model => model.StartHours)

                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Day)

                </th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>




                    <td>
                    </td>
                    <td></td>
                    <td></td>
                    <td>
                        @Html.DisplayFor(modelItem => item.OfficeNum)
                    </td>
                    <td>

                        @Html.DisplayFor(modelItem => item.EndHour)
                    </td>
                    <td>

                        @Html.DisplayFor(modelItem => item.StartHours)
                    </td>


                    <td>

                        @Html.DisplayFor(modelItem => item.Day)
                    </td>
                </tr>
            }
            <tr>
            </tr>
        </table>

    }

    else
    {
    <p class="text-right p-5">لا يوجد ساعات مكتبية </p>    
    
    }




</div>
